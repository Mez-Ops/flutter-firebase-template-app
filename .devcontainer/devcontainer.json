{
	"name": "Flutter Code Space Container",
	"image": "ubuntu:latest",
	"postStartCommand": {
		"apt-get update": "apt-get update -y",
		"brew bundle install": "brew bundle install --file .devcontainer/Brewfile",
		"flutter cache": "flutter precache"
	},
	"features": {
		"ghcr.io/meaningful-ooo/devcontainer-features/homebrew:2": {},
		"ghcr.io/devcontainers/features/github-cli:1": {},
		"ghcr.io/jarrodcolburn/features/flutter-sdk:0": {}
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"dart-code.flutter",
				"ms-python.python",
				"streetsidesoftware.code-spell-checker",
				"ms-python.vscode-pylance",
				"ms-python.pylint",
				"ms-azuretools.vscode-docker",
				"chflick.firecode",
				"toba.vsfire",
				"github.copilot",
				"github.copilot-chat",
				"figma.figma-vscode-extension",
				"ms-vscode-remote.remote-containers",
				"circlecodesolution.ccs-flutter-color",
				"romanrei.material-dark",
				"redhat.vscode-yaml"
			]
		}
	}
}