{
    "name": "mezops-flutter-firebase-container",
    "image": "tylerpjewell/mezops-flutter-firebase-image",
    "containerEnv": {
        "GOOGLE_APPLICATION_CREDENTIALS": "service-account-key.json"
    },
    "postStartCommand": {
        "flutter": "firebase emulators:start",
        "open readme preview": "code README.md"
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "dart-code.dart-code",
                "dart-code.flutter"
            ]
        }
    },
    "portsAttributes": {
        "4400": {
            "label": "Firebase Emulator Hub",
            "onAutoForward": "notify"
        },
        "4500": {
            "label": "Firebase Logging",
            "onAutoForward": "notify"
        },
        "8080": {
            "label": "Firebase Firestore",
            "onAutoForward": "notify"
        },
        "9150": {
            "label": "Firebase Firestore Websocket",
            "onAutoForward": "notify"
        },
        "5000": {
            "label": "Firebase Hosting",
            "onAutoForward": "notify"
        }
    }
}