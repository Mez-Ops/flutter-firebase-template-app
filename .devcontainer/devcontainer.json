  "name": "Flutter",
  "image": "cirrusci/flutter:stable",
  "customizations": {
    "vscode": {
      "extensions": [
        "dart-code.flutter",
        "dart-code.dart-code",
        "ms-python.python",
        "ms-python.vscode-pylance",
        "ms-python.pylint",
        "ms-azuretools.vscode-docker",
        "chflick.firecode",
        "toba.vsfire",
